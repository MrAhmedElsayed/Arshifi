{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:allow-read-dir",
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-mkdir",
    "fs:allow-copy-file",
    "dialog:allow-open",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "$DESKTOP/*"
        },
        {
          "path": "$HOME/**"
        },
        {
          "path": "C:\\**"
        },
        {
          "path": "D:\\**"
        },
        {
          "path": "E:\\**"
        },
        {
          "path": "F:\\**"
        },
        {
          "path": "G:\\**"
        },
        {
          "path": "H:\\**"
        }
      ]
    },
    "shell:default",
    "store:default"
  ]
}